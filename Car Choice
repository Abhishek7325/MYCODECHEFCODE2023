#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    float x1,x2,y1,y2;
	    cin>>x1>>x2>>y1>>y2;
	    float c1=y1/x1;
	    float c2=y2/x2;
	    if(c1<c2){
	        cout<<"-1"<<endl;
	    }
	    else if(c1>c2){
	        cout<<"1"<<endl;
	    }
	    else if(c1==c2){
	        cout<<"0"<<endl;
	    }
	}

}
