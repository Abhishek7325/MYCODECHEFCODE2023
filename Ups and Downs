#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int N;
	    cin>>N;
	    int A[N];
	    for(int i=0;i<N;i++){
	        cin>>A[i];
	    }
	    sort(A,A+N);
	    for(int i=1;i<N-1;i++){
	        swap(A[i],A[i+1]);
	           i++;
	    }
	    for(int i=0;i<N;i++){
	        cout<<A[i]<<" ";
	    }
	    cout<<"\n";
	}

}
