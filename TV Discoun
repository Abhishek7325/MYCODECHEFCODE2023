#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int A,B,C,D;
	    cin>>A>>B>>C>>D;
	    if(A-C<B-D){
	        cout<<"First"<<endl;
	    }
	    else if(A-C>B-D){
	        cout<<"Second"<<endl;
	    }
	    else{
	        cout<<"Any"<<endl;
	    }
	}

}
