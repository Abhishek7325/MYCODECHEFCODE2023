#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int N;
	    cin>>N;
	    if(N%2!=0){
	        cout<<-1<<endl;
	    }
	    else{
	        for(int i=N;i>0;i--){
	            cout << i<<" ";
	        }
	        cout<<endl;
	        
	    }
	    
	}

}
