#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int X,Y,Z;
	    cin>>X>>Y>>Z;
	    if(X<Y&&X<Z){
	        cout<<"NOTHING"<<endl;
	    }
	    else if(X>=Y){
	        cout<<"PIZZA"<<endl;
	    }
	    else{
	        cout<<"BURGER"<<endl;
	    }
	}
	return 0;
}
