#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int N,V1,V2;
	    float X,Y;
	    cin>>N>>V1>>V2;
	    X=(N*1.41421)/V1;
	    Y=(2.0*N)/V2;
	    if(X<Y){
	        cout<<"Stairs"<<endl;
	    }
	    else{
	        cout<<"Elevator"<<endl;
	    }
	    
	}
	return 0;
}
