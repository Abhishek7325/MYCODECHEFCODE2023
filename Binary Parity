#include <iostream>
using namespace std;

int main()
{
	// your code goes here
	int T;
	cin >> T;
	while (T--)
	{
		int N;
		cin >> N;
		int sum = 0;
		while (N / 2 != 0)
		{
			sum += N % 2;
			N = N / 2;
		}
		if ((sum + 1) % 2 == 0)
		{
			cout << "EVEN" << endl;
		}
		else
		{
			cout << "ODD" << endl;
		}
	}
}
